    "@types/howler": "^2.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "esbuild": "^0.17.17",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "release-it": "^15.10.3",
    "rollup": "^3.20.7",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
